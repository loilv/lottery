<odoo>
    <record id="create_purchase_inventory_view_wz" model="ir.ui.view">
        <field name="name">Tạo phiếu nhập</field>
        <field name="model">purchase.inventory.wz</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="date"/>
                </group>
                <footer>
                    <button name="create_purchase" string="Tạo" type="object" class="oe_highlight"/>
                    <button string="Đóng" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>